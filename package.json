{
  "name": "pfych-table",
  "version": "1.0.5",
  "description": "Generate a sortable HTML Table based off of config & a data object",
  "homepage": "https://github.com/pfych/pfych-table",
  "author": "Pfych (https://pfy.ch)",
  "license": "MIT",
  "keywords": [
    "table",
    "html"
  ],
  "main": "dist/index.js",
